{
  "name": "游戏组队平台",
  "description": "基于React+LeanCloud的游戏组队平台",
  "framework": "react",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "nodeVersion": "18",
  "environmentVariables": {},
  "redirects": [
    {
      "source": "/*",
      "destination": "/index.html",
      "statusCode": 200
    }
  ],
  "headers": [
    {
      "source": "/static/**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 